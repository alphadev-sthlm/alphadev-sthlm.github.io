{
  "name": "qs-map-widget",
  "version": "0.0.2",
  "description": "An app for presenting a US elections map",
  "main": "app.js",
  "homepage": "https://github.com/ExpressenAB/qs-map-widget",
  "repository": {
    "type": "git",
    "url": "git@github.com:ExpressenAB/qs-map-widget.git"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "start": "npm run dev",
    "nodemon": "nodemon",
    "ci": "npm install && npm prune && npm ls && npm test",
    "lint": "eslint . --cache && npm run stylint",
    "revision": "which git && [[ -d .git ]] && git rev-parse HEAD > config/_revision || true",
    "test": "mocha && eslint . --cache && npm run stylint",
    "clean-public": "rimraf ./public",
    "create-public": "mkdir -p public/assets",
    "prebuild": "npm-run-all clean-public create-public lint build:vendor",
    "build": "npm-run-all webpack:build build:static",
    "predev": "npm run build",
    "dev": "npm-run-all --parallel nodemon *:watch",
    "build:static": "cpx \"./app/assets/{img,swf}/**/*\" public/assets",
    "build:vendor": "cat ./app/assets/js/vendor/s_code.js > public/assets/s_code.js",
    "eslint": "esw",
    "stylint": "stylint ./app/assets/stylus",
    "eslint:watch": "esw -w",
    "stylint:watch": "npm run stylint -- -w",
    "webpack:build": "webpack --colors",
    "webpack:watch": "npm run webpack:build -- --progress --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Expressen AB",
  "license": "proprietary",
  "private": true,
  "dependencies": {
    "bluebird": "^3.4.6",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "cpx": "^1.3.1",
    "exp-asynccache": "^1.2.1",
    "exp-cachebuster": "^2.1.1",
    "exp-config": "^1.3.1",
    "exp-fetch": "^1.2.0",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "googleapis": "^14.1.0",
    "handlebars": "^4.0.5",
    "lodash": "^4.16.3",
    "log": "^1.4.0",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.10.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.3.0",
    "changelog-parser": "^2.0.2",
    "copy-webpack-plugin": "^4.0.0",
    "cpx": "^1.2.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.6.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-watch": "^2.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "mocha-cakes-2": "^1.1.1",
    "nock": "^8.0.0",
    "npm-run-all": "^3.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "release-it": "^2.3.1",
    "semver": "^5.0.3",
    "sinon": "^1.17.6",
    "stylint": "^1.2.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.2.0",
    "supertest": "^2.0.0",
    "uglifyify": "^3.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "files": [
    "app",
    "config",
    "views",
    "lib",
    "public",
    "test",
    "app.js",
    "cluster.js",
    ".eslintrc",
    ".eslintignore",
    ".stylintrc",
    ".nvmrc"
  ]
}
