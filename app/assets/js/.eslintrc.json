{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "strict": [2, "function"],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "require": false,
    "module": false,
    "Waypoint": false,
    "FB": false,
    "expNative": false
  }
}
